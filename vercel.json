{
  "routes": [
    {
      "src": "/api/auth/login",
      "dest": "/api/auth/login.js"
    },
    {
      "src": "/api/auth/whitelist",
      "dest": "/api/auth/whitelist.js"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api/proxy.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
